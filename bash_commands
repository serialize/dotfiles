cmd_git_get_branch=$(git branch --no-color 2> /dev/null | sed -e '/^[^*]/d' -e 's/* \(.*\)/\1/')
cmd_git_get_count_mod=$(git status 2> /dev/null | grep "modified:" | grep -v untracked | wc -l)
cmd_git_get_count_new=$(git status 2> /dev/null | grep "new file" | wc -l)
cmd_git_get_count_del=$(git status 2> /dev/null | grep "deleted" | wc -l)


